  "jest": {
    "testEnvironment": "jsdom",
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|gif|jpg|jpeg|png)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/app/public/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/app/public/",
      "<rootDir>/dist/",
      "__mocks__"
    ],
    "globals": {
      "localStorage": {
        "appLanguage": "zh"
      }
    }
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "gulp",
    "start": "cd dist & npm start",
    "dev:server": "cd server & node server --dev ",
    "dev:client": "webpack-dev-server --hot --inline --public --content-base=./app/public  --host=0.0.0.0 --port=18080 --history-api-fallback",
    "dev": "start npm run dev:server && start npm run dev:client ",
    "preprod": "npm run build",
    "prod": "npm start"
  },
